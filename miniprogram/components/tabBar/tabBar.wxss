/* components/tabBar/tabBar.wxss */
.tabBar {
  position: fixed;
  width: 100%;
  height: 9%;
  padding-bottom: 25rpx;

  display: flex;
  justify-content: space-around;
  bottom: -4%;

  background-color: #ffffff;
  box-shadow: 0px -1px 5px 0px rgba(0, 0, 0, 0.1); 
  border: 10rpx black;
  font-size: small;
}

.tabBarItem {
  display: inline-block;
}

.tabBarImage {
  height: 50rpx;
  width: 50rpx;
}
